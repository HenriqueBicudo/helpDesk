{
  "name": "helpdesk-system",
  "version": "1.0.0",
  "description": "Sistema de Help Desk com SLA autom√°tico",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "host": "concurrently \"npm run dev:server\" \"npm run dev:host\"",
    "dev:server": "cd server && npx tsx index.ts",
    "dev:client": "cd client && npm run dev",
    "dev:host": "cd client && npm run dev:host",
    "server:dev": "cd server && npx tsx index.ts",
    "client:dev": "cd client && npm run dev",
    "server:start": "cd server && npx tsx index.ts",
    "client:build": "cd client && npm run build",
    "client:preview": "cd client && npm run preview",
    "test:api": "npx tsx test-api-sla.js",
    "test:sla": "npx tsx test-sla-system.ts",
    "test:stats": "set DATABASE_URL=postgresql://helpdesk_user:helpdesk_password@localhost:5432/helpdesk && npx tsx test-stats.js",
    "test:dashboard": "set DATABASE_URL=postgresql://helpdesk_user:helpdesk_password@localhost:5432/helpdesk && npx tsx test-dashboard-data.js",
    "check:sla": "set DATABASE_URL=postgresql://helpdesk_user:helpdesk_password@localhost:5432/helpdesk && npx tsx check-sla-data.js",
    "analyze:db": "set DATABASE_URL=postgresql://helpdesk_user:helpdesk_password@localhost:5432/helpdesk && npx tsx analyze-db.js",
    "clean:db": "set DATABASE_URL=postgresql://helpdesk_user:helpdesk_password@localhost:5432/helpdesk && npx tsx clean-db-safe.js",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:reset": "npx tsx scripts/reset-db.ts",
    "db:seed": "npm run db:push && npm run seed",
    "db:fresh": "npm run db:reset && npm run seed",
    "db:export": "npx tsx scripts/export-db.ts",
    "db:import": "npx tsx scripts/import-db.ts",
    "seed": "npx tsx scripts/seed.ts"
  },
  "keywords": [
    "helpdesk",
    "sla",
    "tickets",
    "support"
  ],
  "author": "HenriqueBicudo",
  "license": "MIT",
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.1.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.22",
    "@types/passport": "^1.0.17",
    "@types/passport-local": "^1.0.38",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.2.0",
    "drizzle-kit": "^0.31.8",
    "tsx": "^4.20.3"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.7",
    "drizzle-zod": "^0.8.2",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "googleapis": "^170.1.0",
    "imap": "^0.8.19",
    "mailparser": "^3.9.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.13",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "postgres": "^3.4.7",
    "studio": "^0.13.5",
    "uuid": "^11.1.0",
    "zod": "^3.25.76"
  }
}
